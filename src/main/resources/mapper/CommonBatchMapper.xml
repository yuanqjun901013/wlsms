<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.web.wlsms.dao.CommonBatchDao">
    <select id="queryTableBySql" parameterType="com.web.wlsms.entity.TableEntity" resultType="java.util.LinkedHashMap">
        ${values}
    </select>

    <insert id="insert" parameterType="com.web.wlsms.entity.TableEntity">
        INSERT INTO ${values}
    </insert>

    <update id="update" parameterType="com.web.wlsms.entity.TableEntity">
        UPDATE
        ${tableName}
        SET
        ${values}
        WHERE
        ${params}
    </update>

    <delete id="delete" parameterType="com.web.wlsms.entity.TableEntity">
        DELETE FROM
        ${tableName}
        WHERE
        ${params}
    </delete>
</mapper>